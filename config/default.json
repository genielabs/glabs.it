{
  "zuix": {
    "build": {
      "input": "./source",
      "output": "./docs",
      "copy": [
        "app",
        "apps",
        "css",
        "images",
        "js",
        ".nojekyll"
      ],
      "ignore": [
        "_inc"
      ],
      "compile": [
        "html",
        "css",
        "js",
        "md",
        "svg"
      ]
    },
    "app": {
      "title": "G-Labs: The open source Factory.",
      "googleSiteId": "UA-92522504-1",
      "resourcePath": "/app/",
      "libraryPath": "https://genielabs.github.io/zkit/lib"
    }
  }
}
